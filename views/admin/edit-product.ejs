                          <div class="col-sm-10">
                            <input type="inputText" name="Description" value="<%=product.Description%>" class="form-control">
                          </div>
                        </div>
                        <div class="row mb-3">
                          <label for="" class="col-sm-2 col-form-label">Image</label>
                          <img style="width: 100px; height:100px;" src="<%=product.Image1%>" alt="" id="imgView">
                          <img style="width: 100px; height:100px;" src="<%=product.Image2%>" alt="" id="imgView">
                          <img style="width: 100px; height:100px;" src="<%=product.Image3%>" alt="" id="imgView">
                          <div class="col-sm-10">
